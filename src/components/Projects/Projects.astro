---
import ProjectCard from "./ProjectCard.astro";

import { projects } from "@/data/projects";
---

<section
  id="projects"
  class="flex flex-col items-center justify-center gap-y-10 max-w-2xl"
>
  <h2 class="title blueunderline">Proyectos</h2>
  {projects.map((item) => <ProjectCard project={item} />)}
</section>
